{
  "plugins": [
    "@typescript-eslint",
    "github"
  ],
  "extends": ["plugin:github/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "camelcase": "off",
    "semi": "off",
    "i18n-text/no-en": "off"
  },
  "env": {
    "node": true,
    "es6": true
  }
}
